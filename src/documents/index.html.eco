--- yaml
layout: 'default'
title: 'Home'
---

  <div id="body" class="with-sidebar">
    <div class="wrapper">
      <div id="main">
        <div>
          <img src="/images/left.jpg">
          <img src="/images/right.png">
        </div>

        <div id="content" class="page-main">

          <% for document in @getCollection('posts').toJSON()[0..4]: %>
            <article id="article-99" class="node node-article node-promoted node-teaser en clearfix" role="article" about="" typeof="sioc:Item foaf:Document">
              <header>
                <h2 property="dc:title" datatype="" class="article-title">
                  <a href="<%= document.url %>"><%= document.title %></a>
                </h2>
                <span class="date"><%= document.date.toShortDateString() %></span>
              </header>
              <section>
                <div class="article-info">
                  <div class="user-info">
                    <div class="avatar-wrapper">
                      <a href="" class="avatar" target="_blank">
                        <img height="100" width="100" src=<%= document.author_image %>>
                      </a>
                    </div>
                    <div class="user-details">
                      <p><a class="username" href="" target="_blank"><%= document.author_name %></a></p>
                      <p>Senior Software Engineer</p>
                    </div>
                    <div class="clear"></div>
                  </div><%= document.summery %>
                  <div class="tags">
                    <div class="clear"></div>
                  </div>
                </div>
              </section>
            </article>
          <% end %>

          <li class="pager-next"><a rel="next" title="show all posts" href="pages/posts.html">Show All</a></li>
          </ul></div>  </div>

      <div id="extra">
          
